# JainFood QA/Staging Environment
# Uses same cloud services as dev but with QA-specific settings

APP_ENV=qa
GIN_MODE=release

PORT=8080

# Database (Neon - use same as dev or separate QA database)
DATABASE_URL=

# Redis (Upstash - use same as dev or separate)
REDIS_URL=

# Security - Change for production!
JWT_SECRET=qa-jwt-secret-change-for-production-min32chars
OTP_SECRET=qa-otp-secret-change-for-production-min32chars

# SMS Service
NOTIFY_SERVICE=smsindiahub
SMSINDIAHUB_API_KEY=
SMSINDIAHUB_SENDER_ID=JAINFO
SMSINDIAHUB_CHANNEL=Trans

# Object Storage (MinIO or Cloud)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=jain-food-media-qa

